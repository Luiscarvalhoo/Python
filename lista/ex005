convidados = ['Maria','Antônio','Fabricio']

# O método "insert" é utilizado para adicionar uma variável dentro de uma lista no local que eu desejo 

convidados.insert(1,'Venê')

for convidado in convidados:
    if convidado == 'Antônio':
        print(f"Você não foi convidado, {convidado}")
    else:
        print(f'Gostaria convidar você para meu jantar, {convidado}')
